@Model List<vdklesson04.Models.VdkCustomer>
    @{
        ViewBag.Title = "danh sanh khach hang";
        //var list = ViewBag.list;
        //var list = Model as List<vdklesson04.Models.VdkCustomer>;
        int stt = 0;
    }

    <h2>danh sanh khach hang</h2>
    <di class="row">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>#</th>
                    <th>ma so</th>
                    <th>ho va ten</th>
                    <th>dia chi</th>
                    <th>nam sinh</th>
                    <th>chuc nang</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    stt++;
                    <tr>
                        <td>@stt</td>
                        <td>@item.CustomerId</td>
                        <td>@item.FirsName @item.LastName</td>
                        <td>@item.Address</td>
                        <td>@item.YearOfBirth</td>
                        <td>
                            <a href="#">Edit</a>
                            <a href="#">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        </div>

